{
  "name": "latitude-embedded-analytics-example",
  "description": "Example on how to integrate a Latitude dashboard in a web application with JWT token",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode client && vite build",
    "preview": "wrangler pages dev ./dist",
    "deploy": "$npm_execpath run build && wrangler pages deploy ./dist"
  },
  "private": true,
  "dependencies": {
    "hono": "^4.1.3",
    "honox": "^0.1.9",
    "@latitude-data/jwt": "0.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@hono/vite-cloudflare-pages": "^0.2.4",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.37",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.0.12",
    "wrangler": "^3.32.0"
  }
}
